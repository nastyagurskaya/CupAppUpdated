@model IEnumerable<CupApp.Models.Cup>
@{
    ViewBag.Title = "Index";
}
@*<link href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" rel="stylesheet" />*@
@section scripts{
    @*<script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>*@

    <script>
        function activatejQueryTable() {
            $("#cupTable").DataTable();
        }

        $(function () {
            activatejQueryTable();
        });

    </script>
}
<div id="index">
    <ul class="nav nav-tabs">
        <li><a data-toggle="tab" href="#firstTab">View All</a></li>
        <li class="active"><a data-toggle="tab" href="#secondTab">Add New</a></li>
    </ul>
    <div class="tab-content">
        <div id="firstTab" class="tab-pane fade in ">@await Html.PartialAsync("ViewAll")</div>
        <div id="secondTab" class="tab-pane fade in active">
            @await Html.PartialAsync("AddOrEdit", new Cup(), new ViewDataDictionary(ViewData))
    </div>
</div>
</div>
<link href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" rel="stylesheet" />
